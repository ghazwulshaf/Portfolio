@model dynamic

@if (Model.ItemsCount != 0)
{
    @for (var i = 1; i < Model.ItemsCount+1; i++)
    {
    <tbody class="leading-8 hover:bg-gray-200">
        <td class="w-1/12 text-center">@i</td>
        <td class="w-5/12">Project @i</td>
        <td class="w-3/12 text-center">Web</td>
        <td class="w-3/12 text-center">
            <div class="flex gap-1 *:grow-0"></div>
            <div class="inline-block text-sm bg-gray-200 rounded px-2 py-1">ASP.NET CORE MVC</div>
        </td>
        <td class="w-3/12 text-center *:ms-2">
            <button class="text-red-500"><i data-feather="trash" class="inline h-4"></i></button>
        </td>
    </tbody>
    }
}
else {
    <tbody class="leading-8 hover:bg-gray-200">
        <td colspan="4" class="text-center">No item found.</td>
    </tbody>
}

<script>
    feather.replace();
</script>