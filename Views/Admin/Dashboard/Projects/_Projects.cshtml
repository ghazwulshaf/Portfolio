@model dynamic

@if (Model.ItemsCount != 0)
{
    @for (var i = 1; i < Model.ItemsCount+1; i++)
    {
    <tbody class="border-y border-y-gray-200 hover:bg-gray-200 align-top [&_td>div]:p-1">
        <td class="w-1/12 text-center">
            <div>
                <span>@i</span>
            </div>
        </td>

        <td class="w-1/12">
            <div>
                <div class="relative w-full aspect-[16/9] bg-gray-200"></div>
            </div>
        </td>

        <td class="w-3/12">
            <div>
                <a href="#">Project @i</a>
            </div>
        </td>

        <td class="w-2/12 text-center">
            <div class="flex justify-center gap-1 *:grow-0">
                <div class="inline-block text-sm bg-gray-200 rounded px-2 py-1">Website</div>
            </div>
        </td>

        <td class="w-2/1">
            <div class="flex justify-center gap-1 *:grow-0">
                <div class="inline-block text-sm bg-gray-200 rounded px-2 py-1">ASP.NET CORE MVC</div>
            </div>
        </td>

        <td class="w-2/12">
            <div class="text-center">
                <span>01/01/2001</span>
            </div>
        </td>

        <td class="w-1/12 text-center *:ms-2">
            <div>
                <button class="text-red-500"><i data-feather="trash" class="inline h-4"></i></button>
            </div>
        </td>
    </tbody>
    }
}
else {
    <tbody class="leading-8 hover:bg-gray-200">
        <td colspan="4" class="text-center">No item found.</td>
    </tbody>
}

<script>
    feather.replace();
</script>